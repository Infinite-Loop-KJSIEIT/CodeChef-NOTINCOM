#include<iostream>
using namespace std;
int main()
{
	int m,n,t;
        cin>>t;
    while(t--)
    {
		cin >>m>> n;
		int a[m],b[n];
		for(int i = 0; i < m; i++)
			cin >> a[i];
		for(int j = 0; j < n; j++)
			cin >> b[j];
 
		int count = 0;
            for(int i = 0; i < m; i++)
                for(int j = 0; j < n; j++)
                    if(a[i] == b[j])
                    {
                        count++;
                    }
        cout<<count<<endl;
    }
	return 0;
}
